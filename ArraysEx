public class ArraysEx {
public static void main (String[] args) {
    int[] arr = new int [10];
    int[] arr2 = new int []{1, 2, 3, 4, 5};
    int[] arr = {1, 2, 3, 4, 5};
    System.out.println(Arrays.toString(arr3));

    int i = 10;
    int t = 15;

    while (i < t) {
        System.out.println(i++);
    }
}

}